<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout"
         xmlns:t="/lib/hudson" xmlns:f="/lib/form" xmlns:c="/lib/credentials">

    <f:entry title="${%Cloud_Name}" field="name">
        <f:textbox default="Aci"/>
    </f:entry>

    <f:entry title="${%Azure_Credential}" field="credentialsId">
        <c:select expressionAllowed="false"/>
    </f:entry>

    <f:entry title="${%Resource_Group}" field="resourceGroup">
        <f:select/>
    </f:entry>

    <f:advanced title="${%Images}" align="left">
        <f:entry title="${%Images}" description="${%Image_Description}">
            <f:repeatableHeteroProperty field="templates" hasHeader="true" addCaption="Add Container Template"
                                        deleteCaption="Delete Container Template"/>
        </f:entry>
    </f:advanced>

    <f:advanced title="${%Private_Registry_Credentials}" align="left">
        <f:entry title="${%Private_Registry_Credentials}">
            <f:repeatableHeteroProperty field="defaultPrivateRegistryCredentials" hasHeader="false" addCaption="${%Add_Private_Registry_Credential}"
                                        deleteCaption="${%Delete_Private_Registry_Credential}"/>
        </f:entry>
    </f:advanced>

    <f:advanced title="${%Environment_Variable}" align="left">
        <f:entry title="${%Environment_Variable}">
            <f:repeatableHeteroProperty field="defaultEnvVars" hasHeader="true" addCaption="${%Add_Environment_Variable}"
                                        deleteCaption="${%Delete_Environment_Variable}"/>
        </f:entry>
    </f:advanced>

    <f:advanced title="${%Volumes}" align="left">
        <f:entry title="${%Volumes}">
            <f:repeatableHeteroProperty field="defaultVolumes" hasHeader="true" addCaption="${%Add_Volume}"
                                        deleteCaption="${%Delete_Volume}"/>
        </f:entry>
    </f:advanced>





</j:jelly>
